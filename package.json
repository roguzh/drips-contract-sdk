{
  "name": "@drips/raffle-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for interacting with Drips Raffle contracts on Sui blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:browser": "tsc -p tsconfig.browser.json",
    "dev": "tsc --watch",
