    "sdk",
    "move",
    "drips",
    "typescript"
  ],
  "author": "Drips Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/drips-raffle-sdk.git"
  },
  "dependencies": {
